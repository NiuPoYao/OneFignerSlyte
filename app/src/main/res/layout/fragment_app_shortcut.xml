<xniuniux.onefignerslyte.SquareRelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/app_shortcut_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="bottom|end"
    tools:context="xniuniux.onefignerslyte.AppShortcutFragment">

    <xniuniux.onefignerslyte.CropImageView
        android:id="@+id/shortcut_background"
        android:layout_height="match_parent"
        android:layout_width="match_parent" />

    <xniuniux.onefignerslyte.CircleListLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/circle_list_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"/>



</xniuniux.onefignerslyte.SquareRelativeLayout>